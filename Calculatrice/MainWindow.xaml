<Window x:Class="Calculatrice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculatrice"
        mc:Ignorable="d"
        Title="Calculatrice" Height="450" Width="800">
    <Grid Background="#FFEEEEEE">


        <Grid.ColumnDefinitions>
        </Grid.ColumnDefinitions>
        <WrapPanel HorizontalAlignment="Left" Orientation="Vertical" VerticalAlignment="Top" Margin="150,136,0,0">
            <Button Content="7" Click="RunInteger" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Yu Gothic UI" FontSize="20" BorderBrush="#FF28369D" Background="White" Foreground="#FF28369D" BorderThickness="3,3,3,3" Padding="10,5,10,5" MinHeight="20" MaxWidth="100" MinWidth="100" Width="50" Height="50"/>
